<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdTop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="16dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdEnd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdBottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="16dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/centerCircle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/gdBottom"
        app:layout_constraintEnd_toEndOf="@id/gdEnd"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/gdStart"
        app:layout_constraintTop_toTopOf="@id/gdTop"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/background" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/human"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/centerCircle"
        app:layout_constraintEnd_toEndOf="@id/centerCircle"
        app:layout_constraintStart_toStartOf="@id/centerCircle"
        app:layout_constraintTop_toTopOf="@id/centerCircle"
        app:srcCompat="@mipmap/sun" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="0"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/laptop"
        android:textColor="@android:color/black"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="0"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="30"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/laptop"
        android:textColor="@android:color/black"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="30"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="60"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/laptop"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="60"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/eat"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="120"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/walking"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="120"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="150"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />


    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/laptop"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="150"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />


    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/laptop"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="120dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="210"
        app:layout_constraintCircleRadius="90dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/wake"
        app:layout_constraintCircle="@+id/human"
        app:layout_constraintCircleAngle="210"
